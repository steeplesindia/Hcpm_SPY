						<Source><![CDATA[
        public int active()
        {
            int ret;
        
            ret = super();
        
            if(SPYPayElementTable.PayElementType == SPYPayElementType::Payroll)
            {
                TabPage_Payroll.enabled(true);
                TabPage_Benefit.enabled(false);
                TabPage_Payroll.fastTabExpanded(FastTabExpanded::Yes);
                TabPage_Benefit.fastTabExpanded(FastTabExpanded::No);
            }
            else if(SPYPayElementTable.PayElementType == SPYPayElementType::Benefit)
            {
                TabPage_Payroll.enabled(false);
                TabPage_Benefit.enabled(true);
                TabPage_Payroll.fastTabExpanded(FastTabExpanded::No);
                TabPage_Benefit.fastTabExpanded(FastTabExpanded::Yes);
            }
        
            return ret;
        }

]]></Source>
