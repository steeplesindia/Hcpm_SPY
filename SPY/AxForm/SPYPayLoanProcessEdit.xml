			<Method>
				<Name>classDeclaration</Name>
				<Source><![CDATA[
[Form]
public class SPYPayLoanProcessEdit extends FormRun implements IDialogable
{
    SPYPayLoanProcessServiceController      payLoanProcessServiceController;
    Query       workerQuery;
    QueryRun    workerQueryRun;
    container   queryPack;
    SPYPayLoanProcessContract       payLoanProcessContract;
    boolean     calledFromMasterProcess, calledFromEOS;

}
]]></Source>
			</Method>
			<Method>
				<Name>formRun</Name>
				<Source><![CDATA[
    public FormRun formRun()
    {
        //Required by the SysOperation framework.
        return this;
    }

]]></Source>
			</Method>
			<Method>
				<Name>closedOk</Name>
				<Source><![CDATA[
    public boolean closedOk()
    {
        boolean                     ret;
    
        ret = super();
    
        if(payLoanProcessContract)
        {
            payLoanProcessContract.parmLegalEntity(LegalEntity_ReferenceGroup.value());
            payLoanProcessContract.parmPayrollPeriodLine(PayrollPeriod_ReferenceGroup.value());
            payLoanProcessContract.parmWorkerQuery(queryPack);
            payLoanProcessContract.parmPayrollParameterState(payLoanProcessServiceController.parmPayrollParameterState());
            payLoanProcessContract.parmEOSRunTable(payLoanProcessServiceController.parmEOSRunTable());
        }
    
        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();
        this.initialize();
    }

]]></Source>
			</Method>
			<Method>
				<Name>initialize</Name>
				<Source><![CDATA[
    void initialize()
    {
        SPYPayrollParameterState    payrollParameterState;
    
        if(element.args().caller() && element.args().caller() is SPYPayLoanProcessServiceController)
        {
            payLoanProcessServiceController = element.args().caller() as SPYPayLoanProcessServiceController;
            if(payLoanProcessServiceController.parmCalledFromEOS())
            {
                calledFromEOS = true;
            }
            else
            {
                payrollParameterState = payLoanProcessServiceController.parmPayrollParameterState();
                if(payrollParameterState && payrollParameterState.parmCalledFromMasterProcess())
                {
                    calledFromMasterProcess = true;
                }
                else
                {
                    workerQuery = this.initApplQuery();
                    workerQueryRun = new QueryRun(workerQuery);
                }
            }
        }
    
        element.setEnable();
        if(payLoanProcessServiceController.getDataContractObject() is SPYPayrollFinalizeProcessContract)
            payLoanProcessContract = payLoanProcessServiceController.getDataContractObject();
    
        element.initializeValue();
    }

]]></Source>
			</Method>
			<Method>
				<Name>initApplQuery</Name>
				<Source><![CDATA[
    Query initApplQuery()
    {
        return new Query(queryStr(SPYHcmWorker_All));
    }

]]></Source>
			</Method>
			<Method>
				<Name>initializeValue</Name>
				<Source><![CDATA[
    public void initializeValue()
    {
        LegalEntity_ReferenceGroup.value(payLoanProcessContract.parmLegalEntity());
        PayrollPeriod_ReferenceGroup.value(payLoanProcessContract.parmPayrollPeriodLine());
        queryPack = payLoanProcessContract.parmWorkerQuery();
    }

]]></Source>
			</Method>
			<Method>
				<Name>initQuery</Name>
				<Source><![CDATA[
    void  initQuery()
    {
        if(workerQueryRun.prompt())
        {
            queryPack = workerQueryRun.pack();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>runBase</Name>
				<Source><![CDATA[
    SPYPayLoanProcessServiceController runBase()
    {//Method must be named runBase() for the integration to the Batch dialog to work.
        return payLoanProcessServiceController;
    }

]]></Source>
			</Method>
			<Method>
				<Name>setEnable</Name>
				<Source><![CDATA[
    public void setEnable()
    {
        if(calledFromEOS)
        {
            Choose.visible(false);
            GroupParameter.visible(false);
        }
        else if(calledFromMasterProcess)
        {
            Choose.visible(false);
        }
    }

]]></Source>
			</Method>
