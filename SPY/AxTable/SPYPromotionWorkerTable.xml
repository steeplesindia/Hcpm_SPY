		<Methods>
			<Method>
				<Name>modifiedField</Name>
				<Source><![CDATA[
    public void modifiedField(FieldId _fieldId)
    {
        HcmPositionDetail   hcmPositionDetail;
        HcmJobDetail        hcmJobDetail;

        super(_fieldId);
        switch (_fieldId)
        {
            case fieldNum(SPYPromotionWorkerTable, ProposedPosition) :
                TransDate   effectiveDate = this.StartDate;
                select validtimestate(effectiveDate) hcmPositionDetail
                    where hcmPositionDetail.Position == this.ProposedPosition;
                select validtimestate(effectiveDate) hcmJobDetail
                    where hcmJobDetail.Job == hcmPositionDetail.Job;
                
                if(hcmJobDetail.SPYCompensationLevelGrade || hcmJobDetail.SPYCompensationLevelBand || hcmJobDetail.SPYCompensationLevelStep)
                {
                    this.ProposedGrade  = hcmJobDetail.SPYCompensationLevelGrade;
                    this.ProposedBand    = hcmJobDetail.SPYCompensationLevelBand;
                    this.ProposedStep     = hcmJobDetail.SPYCompensationLevelStep;
                }
                else
                {
                    this.ProposedGrade  = hcmPositionDetail.SPYCompensationLevelGrade;
                    this.ProposedBand    = hcmPositionDetail.SPYCompensationLevelBand;
                    this.ProposedStep     = hcmPositionDetail.SPYCompensationLevelStep;
                }
                break;
        }
    }

]]></Source>
			</Method>
