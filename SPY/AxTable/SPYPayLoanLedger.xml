	<SourceCode>
		<Declaration><![CDATA[
public class SPYPayLoanLedger extends common
{
}
]]></Declaration>
		<Methods>
			<Method>
				<Name>amountRecovered</Name>
				<Source><![CDATA[
    display SPYAmountRecovered amountRecovered()
    {
        if(this.TransactionCurrencyAmount == this.amountRecoveredTransCur())
            return true;
        else
            return false;
    }

]]></Source>
			</Method>
			<Method>
				<Name>amountRecoveredTransCur</Name>
				<Source><![CDATA[
    display public SPYAmount amountRecoveredTransCur()
    {
        SPYPayLedgerSourceLine      payLedgerSourceLine;
        SPYPayrollLedger            payrollLedger;

        if(this.PayLoanTransType == SPYPayLoanTransType::CashSettlement)
        {
            return abs(this.TransactionCurrencyAmount);
        }

        select sum(TransactionCurrencyAmount) from payrollLedger join payLedgerSourceLine where
        payLedgerSourceLine.TableReference == tableNum(SPYPayLoanLedger) &&
        payLedgerSourceLine.RecordReference == this.RecId &&
        payLedgerSourceLine.PayLedgerSource == payrollLedger.PayLedgerSource &&
        payrollLedger.IsFinalised == true;

        return abs(payrollLedger.TransactionCurrencyAmount);
    }

]]></Source>
			</Method>
		</Methods>
	</SourceCode>
